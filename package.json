{
  "name": "confirm-consultas",
  "version": "1.0.0",
  "description": "Sistema de confirmação de consultas via WhatsApp com agendamento recorrente",
  "type": "module",
  "main": "dist/server.js",
  "keywords": [
    "whatsapp",
    "consultas",
    "agendamento",
    "supabase",
    "cron"
  ],
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend && npm run copy:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "copy:frontend": "node copy-frontend.js",
    "start": "node dist/server.js",
    "postinstall": "cd frontend && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/cors": "^2.8.19",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
